{
  "build": {
    "appId": "io.bagoo.console",
    "category": "public.app-category.travel",
    "win": {
      "target": "squirrel",
      "iconUrl": "http://bagoo.rc-preprod.com/admin/favicon.ico",
      "msi": true
    },
    "linux": {
      "target": "deb"
    },
    "mac": {
      "target": "default"
    }
  },
  "ssh": {
    "ip": "api.rc.preprod",
    "user": "admin",
    "target": "/home/admin/www/api/medias/electron"
  },
  "scripts": {
    "postinstall": "install-app-deps",
    "clear": "rm -rf dist/*",
    "start": " NODE_ENV='prod' electron ./app --enable-logging",
    "updateUpdate": "gulp",
    "compile-linux": "NODE_ENV='prod' && npm run clear && build --linux --x64",
    "compile-mac": "NODE_ENV='prod' && npm run clear && build --mac",
    "dist": "gulp"
  },
  "devDependencies": {
    "devtron": "^1.3.0",
    "electron": "^1.3.3",
    "electron-prebuilt": "^1.2.0",
    "electron-builder": "^5.30.0",
    "electron-log": "^1.1.1",
    "electron-packager": "^7.7.0",
    "gulp": "^3.9.1",
    "gulp-bump": "^2.4.0",
    "gulp-clean": "^0.3.2",
    "gulp-notify": "^2.2.0",
    "gulp-rename": "^1.2.2",
    "gulp-run": "^1.7.1",
    "node-notifier": "^4.6.1"
  },
  "dependencies": {
    "configstore": "^2.1.0"
  }
}
